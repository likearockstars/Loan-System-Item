package model;

import java.time.LocalDate;

public class Book extends Item{
	
	private String ISBN;
	private Integer pageCounts;
	
	public Book(String title, String describe, Double price,LocalDate dateRelease, Author author, String genre, String publisher) {
		
		super(title,describe,price, dateRelease,author,genre,publisher);
	}
	
	public String getISBN() {
		
		return this.ISBN;
	}
	
	public Integer getPageCounts() {
		
		return this.pageCounts;
	}

}
