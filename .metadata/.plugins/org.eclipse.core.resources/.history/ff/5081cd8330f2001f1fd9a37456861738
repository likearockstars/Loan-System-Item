package model;

import java.time.LocalDate;

public class Magazine extends Item {
	
	private Integer numberEdition;
	private Integer numberPages;
	
	/**
	 * @param title
	 * @param describe
	 * @param price
	 * @param dateRelease
	 * @param author
	 * @param genre
	 * @param publisher
	 */
	public Magazine(String title, String describe, Double price, LocalDate dateRelease, Author author, String genre, String publisher) {
		
		super(title,describe,price,dateRelease,author,genre,publisher);
	}
	
	/**
	 * @return Returns the number edition
	 */
	public Integer getNumberEdition() {
		
		return this.numberEdition;
	}
	
	/**
	 * @return Returns the number pages of book
	 */
	public Integer getNumberPages() {
		
		return this.numberPages;
	}
	

	@Override
	public String toString() {
		return "Magazine [numberEdition=" + numberEdition + ", numberPages=" + numberPages + ", toString()="
				+ super.toString() + "]";
	}
	
	
}
